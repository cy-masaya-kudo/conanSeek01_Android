    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/headermenu_status_div"
        android:paddingLeft="@dimen/headermenu_status_div"
