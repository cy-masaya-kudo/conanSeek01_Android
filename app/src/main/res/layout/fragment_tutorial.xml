<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <FrameLayout
        android:id="@+id/tutorial_wrapper"
        android:layout_gravity="center"
        android:layout_width="@dimen/wrapper_width"
        android:layout_height="@dimen/wrapper_height">

        <jp.co.cybird.android.conanseek.common.TutorialCover
            android:id="@+id/tap_cover"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@null"/>


        <ImageView
            android:id="@+id/tachie_image"
            android:layout_width="280dp"
            android:layout_height="360dp"
            android:layout_gravity="bottom|left"/>

        <TextView
            android:id="@+id/fukidashi_text"
            android:layout_width="334dp"
            android:layout_height="116dp"
            android:layout_gravity="left|top"
            android:gravity="center_vertical"
            android:text=""
            android:textColor="#000000"
            android:paddingTop="26dp"
            android:paddingBottom="26dp"
            android:paddingLeft="18dp"
            android:paddingRight="18dp"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="80dp"
            android:layout_marginRight="80dp"
            android:textSize="@dimen/text_size_normal"
            android:background="@mipmap/fukidashi_up_left"/>

        <ImageView
            android:id="@+id/pointer"
            android:layout_width="120dp"
            android:layout_height="120dp"/>

        <ImageView
            android:id="@+id/pinch"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_marginLeft="340dp"
            android:layout_marginTop="160dp"/>

    </FrameLayout>

</FrameLayout>